@model StudentDepartment.Models.CreateStuAndDep
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/jodit/3.1.92/jodit.min.css">
<script src="//cdnjs.cloudflare.com/ajax/libs/jodit/3.1.92/jodit.min.js"></script>

<form asp-controller="Register" asp-action="SetDataInDataBase" method="post" enctype="multipart/form-data">


	<div class="container">
		<div class="form-group">
			<label>StuName:</label>
			<input class="form-control" name="StuName" id="StuName" placeholder="Enter Student Name" />
		</div>

		<div class="form-group">
			<label>DepName:</label>
			<input class="form-control" name="DepName" id="DepName" placeholder="Enter Department Name" />
		</div>
		<div class="form-group">
			<label>CV:</label>
			<input class="form-control" name="CV" id="CV" placeholder="Enter CV" />
		</div>
		<div class="form-group">
			<label class="control-label"> Upload File </label>
			<input type="file" id="FileUrl" name="FileUrl" class="form-control" />

		</div>
		<div class="button">
			<button>Create</button>
		</div>

	</div>
</form>
@section Scripts {
	
	<script>
		var editor = new Jodit('#CV');
	</script>
}

